datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Car {
  brand        String?
  createdAt    DateTime @default(now())
  id           String   @id @default(cuid())
  inProduction Boolean?
  licenceId    String?
  model        String
  updatedAt    DateTime @updatedAt
}
